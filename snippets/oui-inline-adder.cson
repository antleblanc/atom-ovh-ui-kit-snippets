                  </oui-field>
              </oui-inline-adder-field>
              <oui-inline-adder-field>
                  <oui-field label="Field 2">
                      <input type="text" class="oui-input" name="field2" data-ng-model="field2">
                  </oui-field>
              </oui-inline-adder-field>
          </oui-inline-adder-row>
          <oui-inline-adder-row>
              <oui-inline-adder-field>
                  <oui-field label="Field 3">
                      <input type="text" class="oui-input" name="field3" data-ng-model="field3" required>
